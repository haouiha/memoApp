generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma_client"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model MemoCard {
  id        String   @id @default(uuid())
  content   String
  createdBy String   @default("USER")
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
